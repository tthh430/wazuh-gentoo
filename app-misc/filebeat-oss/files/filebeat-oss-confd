# init file : /etc/init.d/filebeat-oss

# Filebeat-OSS user
FB_USER="root"

# Filebeat-OSS group
FB_GROUP="root"

# Filebeat-OSS home
FB_HOME="/usr/share/filebeat"

# Filebeat-OSS configuration directory
FB_CONFIG_DIR="/etc/filebeat"

# Filebeat-OSS data directory
FB_DATA_DIR="/var/lib/filebeat"

# Filebeat-OSS logs directory
FB_LOGS_DIR="/var/log/filebeat"

# PID directory
PID_DIR="/run/filebeat-oss"
